{
  "expo": {
    "name": "Chestnut",
    "slug": "chestnut",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#131620"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.pochyly.chestnut",
      "splash": {
        "backgroundColor": "#131620"
      }
    },
    "platforms": ["ios"],
    "plugins": ["expo-splash-screen"]
  }
}
